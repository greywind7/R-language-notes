# ********************Sorting basics********************
x <- sample(1:20)
x
# sort() function sorts, decreasing attribute for reverse order
sort(x)
sort(x decreasing = T)
sort(x, decreasing = T)
# when there are missing values, you can put them at the end using na.last attr.
# sorting a dataframe using a column
y <- data.frame(v1 = c(2,1,3,5,2,6,3), v2 = sample(11:17), v3 = sample(111,117))
# when there are missing values, you can put them at the end using na.last attr.
# sorting a dataframe using a column
y <- data.frame(v1 = c(2,1,3,5,2,6,3), v2 = sample(11:17), v3 = sample(111:117))
y
y[order(y$v1),]
?order
y[order(y$v1,y$v2),]
install.packages("plyr")
# Same thing can be done using plyr
library(plyr)
arrange(z,v1)
z <- y
y[order(y$v1,y$v2),]
# Same thing can be done using plyr
library(plyr)
arrange(z,v1)
arrange(z,v1,v2)
arrange(z,desc(v1),desc(v2))
# when there are missing values, you can put them at the end using na.last attr.
# sorting a dataframe using a column calues using order()
y <- data.frame(v1 = c(2,1,3,5,2,6,3), v2 = sample(11:17, replace = T), v3 = sample(111:117, rep = T))
z <- y
y[order(y$v1,y$v2),]
# when there are missing values, you can put them at the end using na.last attr.
# sorting a dataframe using a column calues using order()
y <- data.frame(v1 = sample(1:7, rep = T), v2 = sample(11:17, rep = T), v3 = sample(111:117, rep = T))
z <- y
y[order(y$v1,y$v2),]
# Same thing can be done using plyr, for decreasing, desc fn
library(plyr)
arrange(z,v1)
arrange(z,desc(v1),desc(v2))
# ********************Add rows/cols********************
z$v4 <- int(rnorm(7))
# ********************Add rows/cols********************
z$v4 <- round(rnorm(7,10,1))
z
# ********************Add rows/cols********************
z$v4 <- round(rnorm(7,10,3))
z
# ********************Add rows/cols********************
z$v4 <- round(rnorm(7,10,7))
z
# v4 didnt exist before but we added so it does now
